{
    "jsc": {
        "target": "es2022",
        "parser": {
            "syntax": "ecmascript",
            "jsx": false
        },
        "baseUrl": "./",
        "paths": {
            "@modules/*": ["src/modules/*"],
            "@src/*": ["src/*"]
        },
        "minify": {
            "compress": true
        }
    },
    "minify": true,
    "module": {
        "type": "commonjs"
    }
}