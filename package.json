{
  "name": "minlog-js",
  "version": "1.0.0",
  "description": "A simple event logger",
  "license": "ISC",
  "author": "Marcus Xie",
  "type": "module",
  "main": "./build/cjs/index.cjs",
  "module": "./build/mjs/index.js",
  "scripts": {
    "build:cjs": "rollup -c --output.file build/cjs/index.cjs --output.format cjs",
    "build:mjs": "rollup -c --output.file build/mjs/index.js --output.format esm",
    "build": "rollup -c",
    "clean": "rm -rf build"
  },
  "exports": {
    ".": {
      "import": "./build/mjs/index.js",
      "require": "./build/cjs/index.cjs"
    }
  },
  "files": [
    "build/mjs",
    "build/cjs"
  ],
  "dependencies": {
    "chalk": "^5.4.1",
    "moment": "^2.30.1",
    "sprintf-js": "^1.1.3",
    "strip-ansi": "^7.1.0",
    "traverse": "^0.6.11"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@swc/cli": "^0.7.3",
    "@swc/core": "^1.11.24",
    "rollup": "^4.40.1"
  },
  "keywords": [
    "nptu",
    "minlog",
    "minlog-js",
    "MinlogJS"
  ]
}
